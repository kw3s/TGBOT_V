# Environment Variables Template
# Copy this file to .env and fill in your values
# DO NOT commit .env to git!

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Deemix Services (Render.com and Railway.app)
DEEMIX_SERVICE_URL_PRIMARY=https://your-render-service.onrender.com
DEEMIX_SERVICE_URL_SECONDARY=https://your-railway-service.up.railway.app

# Deezer (for cover art and ARL-based downloads - legacy, now using Deemix service)
DEEZER_ARL=your_deezer_arl_cookie

# Spotify (for cover art)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Optional: Proxy URLs (comma-separated)
# PROXY_URL=http://proxy1.com:8080,http://proxy2.com:8080
